       BLOCK "040904b0"
       BEGIN
           VALUE "CompanyName", "@PADREND_AUTHORS@\0"
           VALUE "FileDescription", "@PADREND_DESCRIPTION@\0"
           VALUE "FileVersion", "@PADREND_VERSION@\0"
           VALUE "OriginalFilename", "PADrend.exe\0"
           VALUE "ProductName", "PADrend\0"
           VALUE "ProductVersion", "@PADREND_VERSION@\0"
       END
   END
   BLOCK "VarFileInfo"
   BEGIN
       VALUE "Translation", 0x409, 1200
   END
END
